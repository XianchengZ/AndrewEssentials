mponents/Header.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/components/Footer.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/screens/HomeScreen.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/components/Product.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/components/Rating.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/screens/ProductScreen.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/store.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/reducers/productReducers.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/constants/productConstants.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/actions/productActions.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/components/Loader.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/components/Message.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/screens/CartScreen.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/reducers/cartReducers.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/constants/cartConstants.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/actions/cartActions.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/reducers/userReducers.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/constants/userConstants.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/screens/LoginScreen.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/components/FormContainer.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/actions/userActions.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/screens/RegisterScreen.js",["143"],"import React, { useState, useEffect } from 'react';\nimport { Link, Redirect } from 'react-router-dom';\nimport { Form, Button, Row, Col } from 'react-bootstrap';\nimport { useDispatch, useSelector } from 'react-redux';\nimport Message from '../components/Message';\nimport Loader from '../components/Loader';\nimport FormContainer from '../components/FormContainer';\nimport { register } from '../actions/userActions';\n\nconst RegisterScreen = ({ location, history }) => {\n  const [name, setName] = useState('');\n  const [email, setEmail] = useState('');\n  const [password, setPassword] = useState('');\n  const [confirmPassword, setConfirmPassword] = useState('');\n  const [message, setMessage] = useState(null);\n\n  const dispatch = useDispatch();\n\n  const userRegister = useSelector((state) => state.userRegister);\n  const { loading, error, userInfo } = userRegister;\n\n  const redirect = location.search ? location.search.split('=')[1] : '/';\n\n  useEffect(() => {\n    if (userInfo) {\n      history.push(redirect);\n    }\n  }, [history, userInfo, redirect]);\n\n  const submitHandler = (e) => {\n    e.preventDefault();\n    if (password !== confirmPassword) {\n      setMessage('Password Do Not Match');\n    } else {\n      dispatch(register(name, email, password));\n    }\n  };\n\n  return (\n    <FormContainer>\n      <h1>Sign Up</h1>\n      {message && <Message variant=\"danger\">{message}</Message>}\n      {error && <Message variant=\"danger\">{error}</Message>}\n      {loading && <Loader />}\n      <Form onSubmit={submitHandler}>\n        <Form.Group controlId=\"name\">\n          <Form.Label>Name</Form.Label>\n          <Form.Control\n            type=\"name\"\n            placeholder=\"Enter Name\"\n            value={name}\n            onChange={(e) => setName(e.target.value)}\n          ></Form.Control>\n        </Form.Group>\n        <Form.Group controlId=\"email\">\n          <Form.Label>Email Address</Form.Label>\n          <Form.Control\n            type=\"email\"\n            placeholder=\"Enter Email\"\n            value={email}\n            onChange={(e) => setEmail(e.target.value)}\n          ></Form.Control>\n        </Form.Group>\n        <Form.Group controlId=\"password\">\n          <Form.Label>Password</Form.Label>\n          <Form.Control\n            type=\"password\"\n            placeholder=\"Enter Password\"\n            value={password}\n            onChange={(e) => setPassword(e.target.value)}\n          ></Form.Control>\n        </Form.Group>\n\n        <Form.Group controlId=\"confirmPassword\">\n          <Form.Label>Confirm Password</Form.Label>\n          <Form.Control\n            type=\"password\"\n            placeholder=\"Confirm Password\"\n            value={confirmPassword}\n            onChange={(e) => setConfirmPassword(e.target.value)}\n          ></Form.Control>\n        </Form.Group>\n        <Button type=\"submit\" variant=\"primary\" className=\"mt-3\">\n          Register\n        </Button>\n      </Form>\n\n      <Row className=\"py-3\">\n        <Col>\n          Have an Account?{' '}\n          <Link to={redirect ? `/login?redirect=${redirect}` : '/login'}>\n            Login\n          </Link>\n        </Col>\n      </Row>\n    </FormContainer>\n  );\n};\n\nexport default RegisterScreen;\n","/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/screens/ProfileScreen.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/screens/ShippingScreen.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/components/CheckoutSteps.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/screens/PaymentScreen.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/screens/PlaceOrderScreen.js",["144"],"import React, { useState, useEffect } from 'react';\nimport { Button, Row, Col, ListGroup, Image, Card } from 'react-bootstrap';\nimport { Link } from 'react-router-dom'\nimport { useDispatch, useSelector } from 'react-redux';\nimport Message from '../components/Message'\nimport { CheckoutSteps } from '../components/CheckoutSteps'\nimport { createOrder } from '../actions/orderActions'\n\nconst PlaceOrderScreen = ({ history}) => {\n  const dispatch = useDispatch()\n\n\tconst cart = useSelector((state) => state.cart);\n  \n    const addDecimals = (number) => {\n      return (Math.round(number * 100) / 100).toFixed(2)\n    }\n  \n  // Calculate Prices\n  cart.itemPrice = cart.cartItems.reduce((acc, item) => acc + item.price * item.qty, 0)\n  cart.shippingPrice = addDecimals(cart.itemPrice > 100 ? 0 : 30)\n  cart.taxPrice = addDecimals(Number(0.13 * cart.itemPrice))\n  cart.totalPrice = addDecimals(Number(cart.itemPrice) + Number(cart.shippingPrice) + Number(cart.taxPrice))\n\n    const orderCreate = useSelector(state => state.orderCreate)\n    const { order, success, error } = orderCreate\n\n    useEffect(() => {\n      if (success) {\n        history.push(`order/${order._id}`)\n      } \n      // eslint-disable-next-line\n    }, [history, success])\n\n  const placeOrderHandler = () => {\n    dispatch(createOrder({\n      orderItems: cart.cartItems,\n      shippingAddress: cart.shippingAddress,\n      paymentMethod: cart.paymentMethod,\n      itemsPrice: cart.itemsPrice,\n      shippingPrice: cart.shippingPrice,\n      taxPrice: cart.taxPrice,\n      totalPrice: cart.totalPrice,\n    }))\n  }\n\n\treturn <>\n        <CheckoutSteps step1 step2 step3 step4 />\n        <Row>\n          <Col md={8}>\n            <ListGroup variant='flush'>\n              <ListGroup.Item>\n                <h2>Shipping</h2>\n                <p>\n                  <strong>Address: </strong>\n                  {cart.shippingAddress.address},{' '}\n                  {cart.shippingAddress.city},{' '}\n                  {cart.shippingAddress.postalCode},{' '}\n                  {cart.shippingAddress.country}\n\n                </p>\n              </ListGroup.Item>\n              \n              <ListGroup.Item>\n                <h2>Payment Method</h2>\n                <p>\n                  <strong>Method: </strong>\n                  {cart.paymentMethod}\n                </p>\n              </ListGroup.Item>\n              \n              <ListGroup.Item>\n                <h2>Order Items:</h2>\n                {cart.cartItems.length === 0 ? <Message>Your Cart is empty</Message> :\n                <ListGroup variant='flush'>\n                  {cart.cartItems.map((item, index) => (<ListGroup.Item key={index}>\n                    <Row>\n                      <Col md={1}>\n                        <Image src={item.image} alt={item.name} fluid rounded />\n                      </Col>\n                      <Col>\n                        <Link to={`/product/${item.product}`}>\n                          {item.name}\n                        </Link>\n                      </Col>\n                      <Col md={4}>\n                        {item.qty} * ${item.price} = ${addDecimals(item.qty * item.price)}\n                      </Col>\n                    </Row>\n                  </ListGroup.Item>))}\n                  </ListGroup>}\n              </ListGroup.Item>\n            </ListGroup>\n          </Col>\n          <Col md={4}>\n            <Card>\n              <ListGroup variant='flush'>\n                <ListGroup.Item>\n                  <h2>Order Summary</h2>\n                </ListGroup.Item>\n                <ListGroup.Item>\n                    <Row>\n                      <Col>Items</Col>\n                      <Col>${' '}{cart.itemPrice}</Col>\n                    </Row>\n                </ListGroup.Item>\n                \n                <ListGroup.Item>\n                    <Row>\n                      <Col>Shipping</Col>\n                      <Col>${' '}{cart.shippingPrice}</Col>\n                    </Row>\n                </ListGroup.Item>\n                \n                <ListGroup.Item>\n                    <Row>\n                      <Col>Tax</Col>\n                      <Col>${' '}{cart.taxPrice}</Col>\n                    </Row>\n                </ListGroup.Item>\n\n                <ListGroup.Item>\n                    <Row>\n                      <Col>Total</Col>\n                      <Col>${' '}{cart.totalPrice}</Col>\n                    </Row>\n                </ListGroup.Item>\n\n                <ListGroup.Item>\n                  {error && <Message variant='danger'>{error}</Message>}\n                </ListGroup.Item>\n\n                <ListGroup.Item>\n                    <Button type='button' \n                    className='w-100' disabled={cart.cartItems === 0} onClick={placeOrderHandler} >Place Order</Button>\n                </ListGroup.Item>\n\n              </ListGroup>\n            </Card>\n          </Col>\n        </Row>\n  </>;\n};\n\nexport default PlaceOrderScreen;\n","/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/reducers/orderReducers.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/constants/orderConstants.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/actions/orderActions.js",[],"/Users/andrewzang/programming/JavaScript/projects/AndrewsEssential_new/frontend/src/screens/OrderScreen.js",[],{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","severity":1,"message":"150","line":2,"column":16,"nodeType":"151","messageId":"152","endLine":2,"en